{
  "name": "npm-starter-file",
  "version": "0.0.4",
  "author": "Dan Blake <dan@designbyblake.com> (http://www.designbyblake.com)",
  "license": "MIT",
  "description": "Starter Files for NPM Scripts",
  "dependencies": {
    "phantomjs-prebuilt": "^2.1.4",
    "casperjs": "^1.1.2",
    "resemblejs": "^2.2.2"
  },
  "devDependencies": {
    "ally": "^0.2.13",
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.22.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.7",
    "browserify": "^13.1.1",
    "clean-css": "^4.0.5",
    "clean-css-cli": "^4.0.5",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.2.0",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-nibble": "^3.1.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-combine-mq": "^0.4.0",
    "gulp-file-include": "^1.0.0",
    "gulp-html-prettify": "0.0.1",
    "gulp-html-validator": "0.0.5",
    "gulp-htmlhint": "^0.3.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.3.0",
    "gulp-watch": "^4.3.11",
    "htmlhint-stylish": "^1.0.3",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "minifier": "^0.8.1",
    "node-sass": "^4.5.0",
    "onchange": "^3.2.1",
    "phantomcss": "^1.1.5",
    "postcss-cli": "^3.0.0-beta",
    "reveal.js": "^3.4.1",
    "svgo": "^0.7.2",
    "uglifyjs": "^2.4.10"
  },
  "scripts": {
    "scss": "node-sass --output-style expanded --indent-type tab --indent-width 1 -o build/_temp build/_sass",
    "combineMq": "gulp combineMq",
    "build:css": "npm run scss && npm run combineMq",
    "minify:css": "npm run build:css && cleancss -o assets/css/screen.css assets/css/screen.css",
    "watch:css": "onchange \"build/_sass/**/*.scss\" -- npm run build:css",
    "build:js": "browserify build/_js/_custom/scripts.js -d -o assets/js/scripts.js -t [ babelify --presets [ es2015 ] ]",
    "build:js:prod": "browserify build/_js/_custom/scripts.js -o assets/js/scripts.js -t [ babelify --presets [ es2015 ] ] && minify -o assets/js/scripts.js assets/js/scripts.js",
    "js:scripts": "cd build/_js/_lib && uglifyjs -o ../../assets/js/plugins.js",
    "watch:js": "onchange \"build/_js/**/*.js\" -- npm run build:js",
    "minify:js": "npm run js:scripts && npm run build:js:prod",
    "eslint": "eslint \"build/_js/_custom/**\"",
    "nibble": "eslint-nibble \"build/_js/_custom/**\"",
    "images": "node build/scripts/image.js && npm run svg:min",
    "images:move": "node build/scripts/move-images.js",
    "svg:min": "svgo -f build/_images/ -o assets/images/",
    "gulp:build": "gulp fileinclude && gulp templates",
    "watch:html": "onchange \"build/_html/**/*.html\" -- npm run gulp:build",
    "prod": "npm run minify:css && npm run minify:js",
    "sync": "cd public && browser-sync start --server --files \"**/**/*.*\"",
    "watch:all": "concurrently \"npm run watch:css\" \"npm run watch:js\" \"npm run watch:html\"",
    "start": "concurrently \"npm run sync\" \"npm run watch:all\"",
    "test:home": "casperjs test tests/lmohome.js --engine=slimerjs",
    "test": "bin/test.sh"
  }
}
